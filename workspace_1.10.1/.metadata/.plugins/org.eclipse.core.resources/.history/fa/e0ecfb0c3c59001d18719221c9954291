#include <ConfingSystemClass.h>

//anything you want to attach before infinite loops out here
//nots Only Confing
void ConfingSystem::InitApp() {
	HAL_Init();
	SystemClock_Config();
	this->MX_GPIO_Init();
	this->MX_TIM1_Init();
	this->MX_I2C1_Init();
}

